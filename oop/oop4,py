from dataclasses import dataclass
class Thing:
 def __init__(self, name, size):
    self.name = name
    self.size=size
 def __str__(self):
     return f"object {self.name} with size {self.size}"
 
thing = Thing("pisak", 10)
print (thing)

'''dataclass '''
@dataclass
class Record():
    x: str
    y: int

rec = Record("noc",20)
print(rec.x)
print(rec.y)